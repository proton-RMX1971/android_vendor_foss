android_app_import {
        name: "AdAway",
        apk: "prebuilt/AdAway/AdAway.apk",
        presigned: true,
        dex_preopt: {
                enabled: false,
        },
}

android_app_import {
        name: "BlackHole",
        apk: "prebuilt/BlackHole/BlackHole.apk",
        presigned: true,
        dex_preopt: {
                enabled: false,
        },
}

android_app_import {
        name: "BromiteSystemWebview",
        required: [
            "libwebviewchromium_loader",
            "libwebviewchromium_plat_support",
        ],
        apk: "prebuilt/BromiteSystemWebview/BromiteSystemWebview.apk",
        overrides: ["webview"],
        presigned: true,
        dex_preopt: {
                enabled: false,
        },
}

android_app_import {
        name: "DuckDuckGo",
        apk: "prebuilt/DuckDuckGo/DuckDuckGo.apk",
        overrides: ["Browser2", "Jelly"],
        presigned: true,
        dex_preopt: {
                enabled: false,
        },
}

android_app_import {
        name: "OrganicMaps",
        apk: "prebuilt/OrganicMaps/OrganicMaps.apk",
        presigned: true,
        dex_preopt: {
                enabled: false,
        },
}

android_app_import {
        name: "Droidify",
        apk: "prebuilt/Droidify/Droidify.apk",
        presigned: true,
        dex_preopt: {
                enabled: false,
        },
        required: ["privapp-permissions-droidify.xml"],
        privileged: true,
}

prebuilt_etc_xml {
    name: "privapp-permissions-droidify.xml",
    src: "prebuilt/Droidify/privapp-permissions-droidify.xml",
    filename_from_src: true,
	sub_dir: "permissions",
}

android_app_import {
        name: "AuroraStore",
        apk: "prebuilt/AuroraStore/AuroraStore.apk",
        presigned: true,
        dex_preopt: {
                enabled: false,
        },
        required: ["privapp-permissions-aurorastore.xml"],
        privileged: true,
}

prebuilt_etc_xml {
    name: "privapp-permissions-aurorastore.xml",
    src: "prebuilt/AuroraStore/privapp-permissions-aurorastore.xml",
    filename_from_src: true,
	sub_dir: "permissions",
}

android_app_import {
        name: "Telegram",
        apk: "prebuilt/Telegram/Telegram.apk",
        presigned: true,
        dex_preopt: {
                enabled: false,
        },
}