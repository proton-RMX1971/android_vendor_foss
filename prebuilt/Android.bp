android_app_import {
        name: "AdAway",
        apk: "AdAway/AdAway.apk",
        presigned: true,
        dex_preopt: {
                enabled: false,
        },
}

android_app_import {
        name: "BlackHole",
        apk: "BlackHole/BlackHole.apk",
        presigned: true,
        dex_preopt: {
                enabled: false,
        },
}

android_app_import {
        name: "BromiteSystemWebview",
        required: [
            "libwebviewchromium_loader",
            "libwebviewchromium_plat_support",
        ],
        apk: "BromiteSystemWebview/BromiteSystemWebview.apk",
        overrides: ["webview"],
        presigned: true,
        dex_preopt: {
                enabled: false,
        },
}

android_app_import {
        name: "DuckDuckGo",
        apk: "DuckDuckGo/DuckDuckGo.apk",
        overrides: ["Browser2", "Jelly"],
        presigned: true,
        dex_preopt: {
                enabled: false,
        },
}

android_app_import {
        name: "OrganicMaps",
        apk: "OrganicMaps/OrganicMaps.apk",
        presigned: true,
        dex_preopt: {
                enabled: false,
        },
}

android_app_import {
        name: "Droidify",
        apk: "Droidify/Droidify.apk",
        presigned: true,
        dex_preopt: {
                enabled: false,
        },
        required: ["privapp-permissions-droidify.xml"],
        privileged: true,
}

prebuilt_etc_xml {
        name: "privapp-permissions-droidify.xml",
        src: "Droidify/privapp-permissions-droidify.xml",
        filename_from_src: true,
	sub_dir: "permissions",
}

android_app_import {
        name: "AuroraStore",
        apk: "AuroraStore/AuroraStore.apk",
        presigned: true,
        dex_preopt: {
                enabled: false,
        },
        required: ["privapp-permissions-aurorastore.xml"],
        privileged: true,
}

prebuilt_etc_xml {
        name: "privapp-permissions-aurorastore.xml",
        src: "AuroraStore/privapp-permissions-aurorastore.xml",
        filename_from_src: true,
	sub_dir: "permissions",
}

android_app_import {
        name: "Telegram",
        apk: "Telegram/Telegram.apk",
        presigned: true,
        dex_preopt: {
                enabled: false,
        },
}

android_app_import {
        name: "Insular",
        apk: "Insular/Insular.apk",
        presigned: true,
        dex_preopt: {
                enabled: false,
        },
        required: ["privapp-permissions-insular.xml"],
}

prebuilt_etc_xml {
        name: "privapp-permissions-insular.xml",
        src: "Insular/privapp-permissions-insular.xml",
        filename_from_src: true,
        sub_dir: "permissions",
}